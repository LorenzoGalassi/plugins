{
  "name": "datocms-plugin-tiny-mce",
  "homepage": "https://github.com/YOUR-USER/datocms-plugin-tiny-mce",
  "version": "0.0.1",
  "description": "Custom WYSIWYG editor",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --port 5000",
    "dist": "NODE_ENV=production webpack --progress",
    "addToProject": "yo datocms-plugin:add-to-project",
    "prepublishOnly": "rimraf lib dist && mkdir dist && npm run dist",
    "lint": "eslint \"./src/*.{js,jsx}\"",
    "test": "echo 1"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "text-field"
  ],
  "files": [
    "dist",
    "docs"
  ],
  "author": "DatoCMS <support@datocms.com>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Tiny Mce",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.png",
    "entryPoint": "dist/index.html",
    "pluginType": "field_editor",
    "fieldTypes": [
      "text"
    ],
    "parameters": {
      "global": [],
      "instance": []
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "generator-datocms-plugin": "^1.1.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.2",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "serve": "^12.0.1",
    "style-loader": "^3.3.0",
    "webpack": "^5.58.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "yo": "^4.3.0"
  },
  "dependencies": {
    "html-webpack-tags-plugin": "^3.0.1"
  }
}
